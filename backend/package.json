{
  "name": "catch-up",
  "version": "4.1.0",
  "description": "Catchup",
  "scripts": {
    "build:dev": "tsc --watch --preserveWatchOutput --pretty",
    "start:dev": "cross-env NODE_ENV=development nodemon | pino-colada",
    "build:prod": "pnpm check:ts && pnpm check:eslint && rimraf dist && tsc",
    "start:prod": "node dist/src/index.js",
    "check:ts": "tsc --noEmit",
    "check:eslint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "ts-patch install"
  },
  "license": "MIT",
  "dependencies": {
    "@mobily/ts-belt": "^4.0.0-rc.5",
    "dotenv": "^16.3.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "exhaustive": "^1.1.1",
    "fastify": "^4.24.0",
    "fastify-typeorm-plugin": "^2.2.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "typeorm": "^0.3.17",
    "typia": "^5.2.6"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.6",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "cli-color": "^2.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fastify-tsconfig": "^2.0.0",
    "nodemon": "^3.0.1",
    "pino-colada": "^2.2.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.3.0",
    "type-fest": "^4.8.2",
    "typescript": "^5.2.2"
  }
}